                <TitledPane fx:id="folderTree" animated="false" maxHeight="1.7976931348623157E308" text="Folder Name">
                     <content>
                        <TreeView prefHeight="200.0" prefWidth="200.0" />
                     </content></TitledPane>
               </children>
            </VBox>
            <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="File Name">
                     <content>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <FlowPane maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" VBox.vgrow="NEVER">
                                 <children>
                                    <Hyperlink text="Folder" />
                                    <Label text="&gt;" />
                                    <Hyperlink text="Folder" />
                                    <Label text="&gt;" />
                                    <Hyperlink text="File" />
                                 </children>
                              </FlowPane>
                              <Pane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                                 <children>
                                    <ImageView fx:id="imageView" fitHeight="496.0" fitWidth="567.0" layoutX="1.0" layoutY="-2.0" onDragDropped="#imageViewDropped" onDragOver="#imageViewDragOver" pickOnBounds="true" preserveRatio="true" />
                                 </children></Pane>
                           </children>
                        </VBox>
                     </content>
                </Tab>
              </tabs>
            </TabPane>
            <VBox SplitPane.resizableWithParent="false">
               <children>
                <TitledPane animated="false" maxHeight="1.7976931348623157E308" text="Tool Box 1">
                  <content>
                    <AnchorPane maxHeight="1.7976931348623157E308" />
