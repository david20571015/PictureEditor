<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PictureViewerController">
   <children>
      <MenuBar VBox.vgrow="NEVER">
        <menus>
